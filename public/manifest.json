{
    "name": "sample extension",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "permissions": [
        "activeTab",
        "clipboardWrite",
        "declarativeContent",
        "storage",
        "<all_urls>"
    ],
    "options_page": "options.html",
    "commands": {
        "hello": {
            "suggested_key": "Ctrl+Shift+0",
            "description": "Say Hello"
         }
    }
}